"## title: Outputting Input Reversedly
## var othello: a stacky man
## var lady_macbeth: who pushes him around till he pops
## description: The one and only
Act1 {
  ## description: In the beginning, there was nothing
  Scene1 {
    stage(othello, lady_macbeth)

    othello {
      @you.set(0)
    }
  }

  ## description: Pushing to the very end
  Scene2 {
    lady_macbeth {
      @you.read_char
      @you.push_self
    }

    othello {
      @you.set(@you + 1)
      test_eq(othello, -1)
    }

    lady_macbeth {
      if_false(goto(Scene2))
      @you.pop
    }

    othello {
      @you.set(@you - 1)
    }
  }

  ## description: Once you pop, you can't stop
  Scene3 {
    lady_macbeth {
      @you.pop
      @you.print_char
    }

    othello {
      @you.set(@you + -1)
      test_gt(@you, 0)
    }

    lady_macbeth {
      if_true(goto(Scene3))
    }
  }

  ## description: The end
  Scene4 {
    unstage_all
  }
}"
`;
