  Scene1 {
    stage(the_ghost, juliet)

    the_ghost {
      @you.set(8)
      @you.set(square(@you) - 2)
      @you.print_char
    }

    juliet {
      @you.read_int
    }

    unstage(the_ghost)

    stage(romeo)

    juliet {
      @you.set(2)
    }
  }
}

## description: Determining divisibility
Act2 {
  ## description: A private conversation
  Scene1 {
    juliet {
      test_gt(@you, the_ghost)
    }

    romeo {
      if_true(goto(Scene5))
    }

    unstage(romeo)

    stage(hamlet)

    juliet {
      @you.set(square_root(romeo))
    }

    hamlet {
      @you.set(2)
    }
