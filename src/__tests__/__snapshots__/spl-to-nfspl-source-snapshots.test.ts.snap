      @you.print_char
    }

    unstage(romeo)

    stage(juliet)
  }

  ## description: One small dog at a time
  Scene4 {
    unstage(hamlet)

    stage(romeo)

    juliet {
      @you.set(@you + 1)
      goto(Scene1)
