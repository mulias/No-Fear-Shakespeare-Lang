  ## description: Questions and the consequences thereof
  Scene2 {
    juliet {
      test_gt(juliet, @you)
    }

    hamlet {
      if_true(goto(Scene3))
    }

    juliet {
      test_eq(romeo % juliet, 0)
    }

    hamlet {
      if_true(goto(Scene4))
